{
	"name": "<%= moduleName %>",
	"version": "0.1.0",
	"description": "<%= moduleDescription %>",
	"license": "MIT",
	"repository": "rigwild/<%= repoName %>",
	"author": {
		"name": "rigwild",
		"email": "me@rigwild.dev",
		"url": "https://rigwild.dev"
	},<% if (!cli) { %>
	"main": "dist/index.js",
	"types": "dist/index.d.ts",<% } if (cli) { %>
	"bin": {
		"<%= moduleName.replace('-cli', '') %>": "dist/cli.js"
	},<% } %>
	"engines": {
		"node": ">=10"
	},
	"scripts": {<% if (cli) { %>
		"start": "node dist/cli.js",<% } %>
		"build": "tsc",
		"prepublishOnly": "tsc",
		"test": "<% if (cli) { %>tsc && <% } %>ava --verbose"
	},
	"files": [
		"dist"
	],
	"keywords": [<% if (cli) { %>
		"cli-app",
		"cli"<% } %>
	],
	"dependencies": {<% if (cli) { %>
		"meow": "^6.1.0",
		"update-notifier": "^4.1.0"
	<% } %>},
	"devDependencies": {<% if (cli) { %>
		"@types/update-notifier": "^4.1.0",<% } %>
		"ava": "^3.8.1",<% if (cli) { %>
		"execa": "^4.0.0",<% } %>
		"ts-node": "^8.9.1",
		"typescript": "^3.8.3"
	},
	"ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
